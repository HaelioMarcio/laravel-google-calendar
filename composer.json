{
    "name": "haeliomarcio/laravel-google-calendar",
    "description": "Manage events on a Google Calendar",
    "keywords": [
        "haeliomarcio",
        "calendar",
        "google",
        "event",
        "schedule",
        "laravel-google-calendar"
    ],
    "homepage": "https://github.com/spatie/laravel-google-calendar",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "google/apiclient": "^2.2",
        "illuminate/support": "^5.8|^7.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.0",
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "haeliomarcio\\GoogleCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "haeliomarcio\\GoogleCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "haeliomarcio\\GoogleCalendar\\GoogleCalendarServiceProvider"
            ],
            "aliases": {
                "GoogleCalendar": "haeliomarcio\\GoogleCalendar\\GoogleCalendarFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
